{#==================================================
BEGIN GLOBAL META CALCULATIONS
===================================================#}
{%  set siteMetaTitle = config.defaultMetaTitle %}
{%  set siteMetaDescription = config.defaultMetaDescription %}
{%  set siteSocialImage = config.defaultSocialShareImage.one().getUrl() %}

{# addon with page #}
{% if entry.title is not empty and entry.title != "Home" %}
    {%  set siteMetaTitle = entry.title ~ " | " ~ siteMetaTitle %}
{%  endif  %}
{% if entry.metaDescription is not empty %}
    {%  set siteMetaDescription = entry.metaDescription %} {# ~ " | " ~ siteMetaDescription #}
{%  endif  %}


{# seo specifc - use site if not set #}
{% if entry.seoTitle is not empty %}
    {%  set siteSeoTitle = entry.seoTitle %}
{%  else %}
    {%  set siteSeoTitle = siteMetaTitle %}
{% endif %}
{% if entry.seoDescription is not empty %}
    {%  set siteSeoDescription = entry.seoDescription %}
{%  else %}
    {%  set siteSeoDescription = siteMetaDescription %}
{% endif %}
{% if entry.seoImage|length %}
    {%  set siteSocialImage = entry.seoImage.one().getUrl() %}
{%  endif %}

{# remove double // #}
{% set siteSocialImage = siteSocialImage | trim('/') %}


{# specific page overrides #}

{#==================================================
END GLOBAL META CALCULATIONS
===================================================#}




